{
  "manifest_version": 3,
  "name": "Free Tab Extender",
  "version": "1.0",
  "description": "Free Tab Extender",
  "permissions": ["storage", "tabs", "activeTab", "history", "windows", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "panel.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}